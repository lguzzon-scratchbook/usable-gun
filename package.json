{
    "browser": "browser.js",
    "bugs": {
        "url": "https://github.com/atjn/usable-gun/issues"
    },
    "description": "Gun with best practices",
    "devDependencies": {
        "@atjn/eslint-config": "^8.0.0",
        "@babel/core": "^7.25.8",
        "@types/babel__core": "^7.20.5",
        "@types/fs-extra": "^11.0.4",
        "@types/ws": "^8.5.12",
        "babel-plugin-minify-dead-code-elimination": "^0.5.2",
        "chalk": "^5.3.0",
        "eslint": "^8.44.0",
        "fs-extra": "^11.2.0",
        "gun": "^0.2020.1240",
        "lebab": "^3.2.4",
        "prettier": "^3.3.3",
        "strip-ansi": "^7.1.0",
        "tiny-glob": "^0.2.9"
    },
    "engines": {
        "node": ">=20"
    },
    "files": [
        "/index.js",
        "/index.d.ts",
        "/as.js",
        "/axe.js",
        "/gun.js",
        "/sea.js",
        "/usableLib/**/*",
        "/types/**/*",
        "/src/**/*",
        "/sea/**/*",
        "/lib/**/*",
        "/docs/**/*"
    ],
    "homepage": "https://github.com/atjn/usable-gun#readme",
    "keywords": [
        "gun",
        "gundb",
        "wrapper"
    ],
    "license": "(Zlib OR MIT OR Apache-2.0)",
    "main": "index.js",
    "name": "usable-gun",
    "optionalDependencies": {
        "aws-sdk": "*",
        "chokidar": "*",
        "emailjs": "*",
        "formidable": "*",
        "uws": "*",
        "ws": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/atjn/usable-gun.git"
    },
    "scripts": {
        "apis": "bun rebuild/upgradeApis.js",
        "clean": "bun rebuild/cleanup.js",
        "commit": "bun pm cache rm && bun x --bun -y -p https://github.com/lguzzon-scratchbook/ai-commit ai-commit -- --force",
        "commit:all": "bun commit -- --all",
        "copy": "bun rebuild/copySource.js",
        "license": "bun rebuild/updateLicense.js",
        "lint": "bun x --bun eslint . --fix ; bun x --bun -y standard . --fix",
        "lint:nofix": "eslint .",
        "release": "bun run clean && bun run copy && bun run version && bun run license && bun run unbuild && bun run apis && bun run wrap",
        "release:lint": "bun run release && npm run lint",
        "unbuild": "bun rebuild/unbuild.js && bun rebuild/unbuild.js sea && bun rebuild/unbuild.js lib/meta",
        "version": "bun rebuild/updateVersion.js",
        "wrap": "bun rebuild/wrapCode.js"
    },
    "type": "module",
    "types": "index.d.ts",
    "version": "0.202002.124001"
}